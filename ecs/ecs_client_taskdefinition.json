{
    "containerDefinitions": [
        {
            "name": "client",
            "image": "%s.dkr.ecr.us-east-2.amazonaws.com/test-driven-client:prod",
            "essential": true,
            "portMappings": [
                {
                    "name": "client-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/flask-react-client-td",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "flask-react-client-td"
}